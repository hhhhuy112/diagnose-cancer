<div class = "col-md-offset-2 col-md-10">
  <%= render "search_form" %>
</div>
<div class="table-responsive diagnoses-user col-md-12">
  <table class="table table-bordered table-hover">
    <thead class = "header-table-diagnoses">
      <%= render "table_header" %>
    </thead>
      <% if @diagnoses.present? %>
        <tbody class = "diagnoses-content">
          <%= render "diagnose"%>
        </tbody>
      <% end %>
  </table>
  <% if !@diagnoses.present? %>
    <div class = "text-danger">
      <i class="fa fa-fw fa-exclamation-triangle"></i>
      <strong><%= t(".data_not_find") %></strong>
    </div>
  <% end %>
</div>
<center><%= paginate @diagnoses %></center>
</div>
